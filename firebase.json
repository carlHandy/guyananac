{
  "hosting": {
    "site": "mx-seller-portal",
    "public": "dist/seller-portal",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "functions"
  },
  "emulators": {
    "auth": {
      "port": 9091
    },
    "functions": {
      "port": 5002
    },
    "firestore": {
      "port": 8081
    },
    "hosting": {
      "port": 4200
    },
    "ui": {
      "enabled": true
    }
  }
}
